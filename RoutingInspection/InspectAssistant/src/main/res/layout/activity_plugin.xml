<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="vertical"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
        >
    <include layout="@layout/common_title"/>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="vertical"
            android:layout_margin="12dp">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="OBD读取插件"
                    style="@style/common_text"/>
            <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"/>
            <TextView
                    android:id="@+id/tv_obd"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="未安装"
                    style="@style/common_text"/>
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/line"/>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="vertical"
            android:layout_margin="12dp">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="预录入插件"
                    style="@style/common_text"/>
            <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"/>
            <TextView
                    android:id="@+id/tv_ylr"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="未安装"
                    style="@style/common_text"/>
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/line"/>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="vertical"
            android:layout_margin="12dp">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="外廓测量插件"
                    style="@style/common_text"/>
            <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"/>
            <TextView
                    android:id="@+id/tv_wkcl"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="未安装"
                    style="@style/common_text"/>
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/line"/>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:orientation="vertical"
            android:layout_margin="12dp">

        <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="执法记录仪插件"
                    style="@style/common_text"/>
            <View
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:layout_weight="1"/>
            <TextView
                    android:id="@+id/tv_zfjly"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="未安装"
                    style="@style/common_text"/>
        </LinearLayout>

    </LinearLayout>

    <include layout="@layout/line"/>
</LinearLayout>